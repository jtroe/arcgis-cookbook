{
    "arcgis": {
        "version": "11.2",
        "run_as_user": "arcgis",
        "repository": {
            "patches": "/opt/software/archives/patches"
        },
        "server": {
            "install_dir": "/opt",
            "patches": [
                "ArcGIS-112-S-*.tar",
                "ArcGIS-112-S-*.tar.gz"
            ]
        },
        "workflow_manager_server": {
            "patches": [
                "WorkflowManager-11.2-*.tar",
                "WorkflowManager-11.2-*.tar.gz"
            ]
        }
    },
    "run_list": [
        "recipe[arcgis-enterprise::install_patches]",
        "recipe[arcgis-workflow-manager::install_patches]"
    ]
}
